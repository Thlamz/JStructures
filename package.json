{
  "name": "jstructures",
  "version": "1.0.0",
  "description": "A JS data structure library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "license": "MIT",
  "devDependencies": {
    "@types/emscripten": "^1.39.6",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "test": "jest --no-cache",
    "compile": "em++ -o src/structures/assembly/output.js src/structures/assembly/src/heap/Heap.cpp src/structures/assembly/src/heap/NumberHeap.cpp src/structures/assembly/src/Bindings.cpp -s EXPORT_NAME='bindings' -s WASM_ASYNC_COMPILATION=0 -s ALLOW_MEMORY_GROWTH=1 --bind -O3",
    "build": "yarn clean && yarn compile && npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:umd": "webpack --config config/webpack.config.js",
    "build:types": "tsc -p config/tsconfig.types.json",
    "clean": "node tools/cleanBuild.js",
    "lint": "eslint src/** test/** --fix"
  },
  "dependencies": {}
}
