{
  "name": "jstructures",
  "version": "1.0.0",
  "description": "A JS data structure library",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/emscripten": "^1.39.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "babel-plugin-search-and-replace": "^1.1.0",
    "chai": "^4.3.4",
    "eslint": "^8.4.1",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "scripts": {
    "cli": "ts-node src/cli.ts",
    "test": "mocha tests/**/*.test.ts -n loader=ts-node/esm --extension ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "compile": "em++ -o src/structures/assembly/build/output.js src/structures/assembly/lib/Heap.cpp -s EXPORT_ES6=1 -s MODULARIZE=1 -s EXPORT_NAME=\"bindings\" -s WASM_ASYNC_COMPILATION=0 --bind",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  }
}
